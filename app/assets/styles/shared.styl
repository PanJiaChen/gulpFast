//Lost Settings
$flexbox = true;
$gutter = 30px
$rtl = false
@import 'typographic';


//Resets
//Use corysimmons/boy reset
//to gell with lost and typographic
@import '_base/reset/reset.css'
ks-box-sizing-reset()
ks-normalize()

//Base
@import '_base/*'
//Mixins
@import '_base/mixins/*'
//Generated Icons
@import '_generated/*'
//Component Styles
@import '_components/*'
//Page Styles
@import '_pages/*'


//Global
@import '_global.styl'


