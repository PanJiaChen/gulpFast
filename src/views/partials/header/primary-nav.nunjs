{#
  Header Nav
  -> To set active page `{% set activePage = "PageName" %}`
#}

{# Import #}
{% import 'macros/nav.nunjs' as nav %}

{# Body #}
<nav class="header-nav">
  {% for item in headerNav %}
    {{nav.active(activePage = activePage == item.title, item.url, item.title, item.class)}}
  {% endfor %}
</nav>