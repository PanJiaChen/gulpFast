{#
  Header Nav
  -> To set active page `{% set activePage = "PageName" %}`
#}

{# Import #}
{% import 'macros/nav.nunjs' as nav %}

{# Body #}
<nav class="primary-nav">
  {% for item in headerNav %}
    <div class="{{item.class}}_wrapper">
      {{nav.activePjax(activePage = activePage == item.title, item.url, item.title, item.class)}}
    </div>
  {% endfor %}
</nav>